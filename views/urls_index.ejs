<% include partials/header %>

<div class="container" align="center">
  <table class="main-table">
    <% urls.forEach(function (array) { %>
      <tr>
        <td><h2><a href="/urls/<%= array[0] %>"><%= array[0] %></a><td>
          <td class="url-table-items"><a href="<%= array[1] %>"><%= array[1] %></a></h2></td>
        <td class="url-table-ietms">
          <a href="/urls/<%= array[0] %>"<btn class="btn btn-info">
            Edit
          </btn></a>
          <td style="padding-left: 40px; padding-bottom: 1px">
            <form action="/urls/<%= array[0] %>/delete" method="POST">
              <input class="btn btn-danger" type="submit" value="Delete">
            </form>
          </td>
      </tr>
    <% }) %>
  </table>
</div>

<% include partials/footer %>
